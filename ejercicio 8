#include <iostream>
#include <string>
#include <vector>
using namespace std;

string UnirPalabras(vector<string>);

int main() {
    int n;
    cout << "Cuantas palabras? ";
    cin >> n;
    cin.ignore();

    vector<string> palabras(n);
    for (int i = 0; i < n; i++)
        getline(cin, palabras[i]);

    cout << UnirPalabras(palabras) << endl;
    return 0;
}

string UnirPalabras(vector<string> v) {
    string r = "";
    for (int i = 0; i < v.size(); i++) {
        r += v[i];
        if (i < v.size() - 1) r += " ";
    }
    return r;
}
